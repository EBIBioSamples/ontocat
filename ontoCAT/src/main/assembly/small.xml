<assembly>
    <id>small</id>
    <formats>
        <format>jar</format>
    </formats>
    <includeBaseDirectory>true</includeBaseDirectory>
    <dependencySets>
        <dependencySet>
        		<unpack>false</unpack>
            <outputDirectory/>
              <excludes>
                        <exclude>jargs:jargs</exclude>
                        <exclude>log4j:log4j</exclude>
                        <exclude>xpp3:xpp3_min</exclude>
                        <exclude>opencsv:opencsv</exclude>
                        <exclude>owlapi:owlapi</exclude>
                        <exclude>ols:ols-client</exclude>
                        <exclude>axis:axis</exclude>
                        <exclude>jaxrpc:jaxrpc</exclude>
                        <exclude>commons-logging:commons-logging</exclude>
                        <exclude>commons-discovery:commons-discovery</exclude>
                        <exclude>commons-lang:commons-lang</exclude>
                        <exclude>xstream:xstream</exclude>
                      </excludes>
        </dependencySet>
    </dependencySets>
</assembly>
