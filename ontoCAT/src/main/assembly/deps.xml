<!-- 
  Prepares the jar that is used by all the MIAME/MINSEQE end-user package
-->
<assembly>
  <id>deps</id>
  
  <formats>
  	<format>jar</format>
  </formats>
  
  <includeBaseDirectory>false</includeBaseDirectory>
  
	<dependencySets>
		<dependencySet>
		  <!-- Enable only if non-empty <outputFileNameMapping></outputFileNameMapping> -->
		  <unpack>true</unpack>
		  <scope>runtime</scope>
		</dependencySet>
	</dependencySets> 
  
</assembly>